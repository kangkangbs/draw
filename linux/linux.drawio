        <mxCell id="19bWRe63vrNReFquvYGr-72" value="/selinux" style="edgeLabel;html=1;align=center;verticalAlign=middle;resizable=0;points=[];" vertex="1" connectable="0" parent="19bWRe63vrNReFquvYGr-71">
